/**app.wxss**/
page {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 130rpx 0;
  box-sizing: border-box;
}

.overall {
  padding-top: 110rpx;
  padding-left: 15rpx;
  padding-right: 25rpx;
  display: flex;
  height: 25px;
  background-color: white;
}

.left {
  padding-left: 50rpx;
  transform: translate(-50%, -50%);
  height: 48rpx;
  width: 48rpx;
}

.title {
  transform: translate(-50%, -50%);
  font-size: 35rpx;
}

.page-title {
  margin: 0 auto;
  transform: translate(-50%, -50%);
  font-size: 35rpx;
}