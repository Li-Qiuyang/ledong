/* pages/mood/mood.wxss */
page {
  width: 100%;
  background-color: #CAE2FA;
}

.all {
  height: 30px;
  background-color: white;
  padding-top: 70rpx;
  display: flex;
  align-items: center;
  padding-bottom: 10px;
  border-bottom: 1px solid #eee;
}

.left1 {
  height: 50rpx;
  width: 50rpx;
  margin-left: 30rpx;
  margin-right: 10rpx;
}

.title1 {
  font-size: 35rpx;
  margin-right: 190rpx;
}

.pagetitle {
  font-size: 35rpx;
  /* z-index: 1; */
}

.up {
  background-color: rgba(135, 206, 250, 0.3);
  color: rgb(241, 248, 255);
  width: 96%;
  margin-left: 2%;
  height: 160px;
  border-radius: 10px;
  text-align: center;
  line-height: 160px;
  margin-top: 20px;
}

.down {
  background-color: rgba(135, 206, 250, 0.3);
  width: 96%;
  margin-left: 2%;
  height: 300px;
  border-radius: 10px;
  margin-top: 20px;
  display: flex;
  flex-wrap: wrap;
}

.down2 {
  width: 100%;
  height: 290px;
  display: flex;
  flex-wrap: wrap;
}

.item {
  width: 20%;
  text-align: center;
  font-size: small;
  margin-top: 10px;
}

.img {
  width: 40px;
  height: 40px;
}